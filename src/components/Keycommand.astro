---

<footer
  class=" l-phone:hidden m-phone:hidden s-phone:hidden print:hidden bottom-0 fixed text-center backdrop-blur-lg border-2 rounded-lg border-slate-200 inline-block justify-center left-1/2 -translate-x-1/2"
>
  &nbsp Pulsa <kbd>CTRL</kbd> + <kbd>k</kbd> para abrir la paleta de comandos. &nbsp
</footer>
<ninja-keys class="print:hidden" hideBreadcrumbs ></ninja-keys>
<script>
  import { UI } from "@/location/es.json";
  const { nav_bar_ui } = UI;
  const { bar_print } = nav_bar_ui;

  import "ninja-keys";

  document.addEventListener("DOMContentLoaded", () => {
    const ninja = document.querySelector("ninja-keys");

    if (ninja !== null) {
      // Function to update the theme of ninja-keys
      const updateNinjaKeysTheme = () => {
        if (document.documentElement.classList.contains("dark")) {
          ninja.classList.add("dark");
        } else {
          ninja.classList.remove("dark");
        }
      };

      // Run initially to set the correct theme
      updateNinjaKeysTheme();

      // Observe changes in the 'class' attribute of the html element
      const observer = new MutationObserver((mutationsList) => {
        for (const mutation of mutationsList) {
          if (
            mutation.type === "attributes" &&
            mutation.attributeName === "class"
          ) {
            updateNinjaKeysTheme();
          }
        }
      });

      observer.observe(document.documentElement, { attributes: true });

      // Initialize ninja-keys data
      ninja.data = [
        {
          id: "Imprimir",
          title: bar_print,
          hotkey: "ctrl+P",
          icon: `
            <svg xmlns="http://www.w3.org/2000/svg" style="margin-right:10px" width="16" height="16" viewBox="0 0 512 512">
              <path fill="var(--icon-color)" d="M128 0C92.7 0 64 28.7 64 64v96h64V64H354.7L384 93.3V160h64V93.3c0-17-6.7-33.3-18.7-45.3L400 18.7C388 6.7 371.7 0 354.7 0H128zM384 352v32 64H128V384 368 352H384zm64 32h32c17.7 0 32-14.3 32-32V256c0-35.3-28.7-64-64-64H64c-35.3 0-64 28.7-64 64v96c0 17.7 14.3 32 32 32H64v64c0 35.3 28.7 64 64 64H384c35.3 0 64-28.7 64-64V384zM432 248a24 24 0 1 1 0 48 24 24 0 1 1 0-48z"/>
            </svg>`,
          handler: () => {
            window.print();
          },
        },
        {
          id: "Linkedin",
          title: "Visitar el LinkedIn",
          icon: `
            <svg xmlns="http://www.w3.org/2000/svg" style="margin-right:10px" width="16" height="16" viewBox="0 0 512 512">
              <path fill="var(--icon-color)" d="M464 0H48C21.5 0 0 21.5 0 48v416c0 26.5 21.5 48 48 48h416c26.5 0 48-21.5 48-48V48c0-26.5-21.5-48-48-48zM160 416H96V224h64v192zm-32-224h-.1c-21.8 0-35.9-15.2-35.9-34.2 0-19.3 14.6-34.8 37-34.8 22.4 0 36 15.5 36 34.8 0 19-14.1 34.2-37.1 34.2zm288 224h-64V304c0-32-12-53.1-39.9-53.1-21.6 0-34.1 14.5-39.7 28.5-2 4.9-2.4 11.8-2.4 18.6v128h-64V224h63.9v24.1h1c9.1-17.3 25.1-35.5 64-35.5 46.7 0 81.1 30.7 81.1 96.6v128z"/>
            </svg>`,
          handler: () => {
            window.open(
              "https://www.linkedin.com/in/alejandro-ortega-ramirez/"
            );
          },
        },
        {
          id: "DarkMode",
          title: "Modo oscuro",
          hotkey: "ctrl+D",
          icon: `
          <svg xmlns="http://www.w3.org/2000/svg" style="margin-right:10px" width="16" height="16" viewBox="0 0 512 512"><!--!Font Awesome Free 6.5.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.-->
            <path fill="var(--icon-color)" d="M144.7 98.7c-21 34.1-33.1 74.3-33.1 117.3c0 98 62.8 181.4 150.4 211.7c-12.4 2.8-25.3 4.3-38.6 4.3C126.6 432 48 353.3 48 256c0-68.9 39.4-128.4 96.8-157.3zm62.1-66C91.1 41.2 0 137.9 0 256C0 379.7 100 480 223.5 480c47.8 0 92-15 128.4-40.6c1.9-1.3 3.7-2.7 5.5-4c4.8-3.6 9.4-7.4 13.9-11.4c2.7-2.4 5.3-4.8 7.9-7.3c5-4.9 6.3-12.5 3.1-18.7s-10.1-9.7-17-8.5c-3.7 .6-7.4 1.2-11.1 1.6c-5 .5-10.1 .9-15.3 1c-1.2 0-2.5 0-3.7 0c-.1 0-.2 0-.3 0c-96.8-.2-175.2-78.9-175.2-176c0-54.8 24.9-103.7 64.1-136c1-.9 2.1-1.7 3.2-2.6c4-3.2 8.2-6.2 12.5-9c3.1-2 6.3-4 9.6-5.8c6.1-3.5 9.2-10.5 7.7-17.3s-7.3-11.9-14.3-12.5c-3.6-.3-7.1-.5-10.7-.6c-2.7-.1-5.5-.1-8.2-.1c-3.3 0-6.5 .1-9.8 .2c-2.3 .1-4.6 .2-6.9 .4z"/></svg>`,
          handler: () => {
            document.documentElement.classList.toggle("dark");
          },
        },
        {
          id: "lightMode",
          title: "Modo claro",
          hotkey: "ctrl+L",
          icon: `
          <svg xmlns="http://www.w3.org/2000/svg" style="margin-right:10px" width="16" height="16" viewBox="0 0 512 512"><!--!Font Awesome Free 6.5.2 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.-->
            <path fill="var(--icon-color)" d="M375.7 19.7c-1.5-8-6.9-14.7-14.4-17.8s-16.1-2.2-22.8 2.4L256 61.1 173.5 4.2c-6.7-4.6-15.3-5.5-22.8-2.4s-12.9 9.8-14.4 17.8l-18.1 98.5L19.7 136.3c-8 1.5-14.7 6.9-17.8 14.4s-2.2 16.1 2.4 22.8L61.1 256 4.2 338.5c-4.6 6.7-5.5 15.3-2.4 22.8s9.8 13 17.8 14.4l98.5 18.1 18.1 98.5c1.5 8 6.9 14.7 14.4 17.8s16.1 2.2 22.8-2.4L256 450.9l82.5 56.9c6.7 4.6 15.3 5.5 22.8 2.4s12.9-9.8 14.4-17.8l18.1-98.5 98.5-18.1c8-1.5 14.7-6.9 17.8-14.4s2.2-16.1-2.4-22.8L450.9 256l56.9-82.5c4.6-6.7 5.5-15.3 2.4-22.8s-9.8-12.9-17.8-14.4l-98.5-18.1L375.7 19.7zM269.6 110l65.6-45.2 14.4 78.3c1.8 9.8 9.5 17.5 19.3 19.3l78.3 14.4L402 242.4c-5.7 8.2-5.7 19 0 27.2l45.2 65.6-78.3 14.4c-9.8 1.8-17.5 9.5-19.3 19.3l-14.4 78.3L269.6 402c-8.2-5.7-19-5.7-27.2 0l-65.6 45.2-14.4-78.3c-1.8-9.8-9.5-17.5-19.3-19.3L64.8 335.2 110 269.6c5.7-8.2 5.7-19 0-27.2L64.8 176.8l78.3-14.4c9.8-1.8 17.5-9.5 19.3-19.3l14.4-78.3L242.4 110c8.2 5.7 19 5.7 27.2 0zM256 368a112 112 0 1 0 0-224 112 112 0 1 0 0 224zM192 256a64 64 0 1 1 128 0 64 64 0 1 1 -128 0z"/></svg>`,
          handler: () => {
            document.documentElement.classList.remove("dark");
          },
        },
      ];
    }
  });
</script>
<style>
  :root {
    --icon-color-light: black;
    --icon-color-dark: white;
  }

  html {
    --icon-color: var(--icon-color-light);
  }

  html.dark {
    --icon-color: var(--icon-color-dark);
  }

  ninja-keys.dark {
  --ninja-secondary-background-color: rgb(55, 59, 66);
  --ninja-selected-background: rgb(80, 88, 99);
  --ninja-modal-background: #22272e;
  --ninja-footer-background: rgba(80, 88, 99, 0.4);
  --ninja-placeholder-color: #8b949e;
}
</style>
